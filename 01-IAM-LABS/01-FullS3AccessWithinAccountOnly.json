{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "FullS3AccessWithinAccountOnly",
            "Effect": "Allow",
            "Action": "s3:*",
            "Resource": "*",
            "Condition": {
                "StringEquals": {
                    "aws:username": "labs2"
                }
            }
        }
    ]
}